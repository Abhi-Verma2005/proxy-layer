{
  "name": "proxy-layer",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --inspect --trace-warnings --trace-deprecation src/app.ts",
    "dev:tsx": "tsx watch src/app.ts",
    "dev:debug": "node --inspect --loader tsx/esm src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "lint": "eslint --ext .ts src tests",
    "setup:outline": "tsx integrations/_meta/generator.ts outline $OUTLINE_DATABASE_URL",
    "setup:main": "tsx integrations/_meta/generator.ts main DIRECT_URL"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^6.0.2",
    "@types/helmet": "^4.0.0",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/winston": "^2.4.4",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "postgres": "^3.4.3",
    "supertest": "^7.1.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/pg": "^8.15.4",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3"
  }
}